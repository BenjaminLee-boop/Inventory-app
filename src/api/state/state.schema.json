{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "State",
	"description": "A State",
	"required": [
		"name",
		"code"
	],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "The database ID of the country."
		},
		"code": {
			"$id": "#/properties/code",
			"type": "string",
			"title": "The Country Code .",
			"examples": [
				"AU",
				"US",
				"CA"
			]
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "The country name.",
			"examples": [
				"Australia",
				"United States",
				"Canada"
			]
		}
	}
  }