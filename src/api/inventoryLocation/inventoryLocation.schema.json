{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Address",
  "description": "The Address",
  "required": ["name", "description", "image_url"],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Of the location."
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "A Description of the location."
    },
    "image_url": {
      "$id": "#/properties/image_url",
      "type": "string",
      "title": "An Image of the location"
    }
  }
}
